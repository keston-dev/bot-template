{
    "name": "bot-template",
    "description": "A simple template i use for my bot projects.",
    "author": {
        "name": "Keston",
        "email": "contact@kestondev.com"
    },
    "module": "src/index.ts",
    "type": "module",
    "version": "1.0.0",
    "scripts": {
        "dev": "bun .",
        "load-commands": "bun run src/util/loadCommands.ts",
        "delete-commands": "bun run src/util/deleteCommands.ts",
        "docker:build": "docker-compose build",
        "docker:up": "docker-compose up -d",
        "docker:down": "docker-compose down",
        "docker:restart": "docker-compose restart bot",
        "docker:load-commands": "docker-compose run --rm bot load-commands",
        "docker:delete-commands": "docker-compose run --rm bot delete-commands"
    },
    "devDependencies": {
        "@types/bun": "latest",
        "@typescript-eslint/eslint-plugin": "^8.53.1",
        "@typescript-eslint/parser": "^8.53.1",
        "eslint": "^9.39.2",
        "eslint-config-prettier": "^10.1.8",
        "eslint-plugin-compat": "^6.1.0",
        "eslint-plugin-jsx-a11y": "^6.10.2",
        "eslint-plugin-prettier": "^5.5.5",
        "eslint-plugin-promise": "^7.2.1",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "import-sort": "^6.0.0",
        "import-sort-parser-typescript": "^6.0.0",
        "import-sort-style-eslint": "^6.0.0",
        "prettier": "^3.8.1",
        "prettier-plugin-import-sort": "^0.0.7"
    },
    "peerDependencies": {
        "typescript": "^5"
    },
    "dependencies": {
        "chalk": "^5.6.2",
        "discord.js": "^14.25.1",
        "mongoose": "^9.1.5"
    }
}
